{
    "ConnectionStrings": {
        "Default": "data.db"
    },
    "DaemonOptions": {
        "GatewayId": "1",
        "MonitorEverySeconds": "5"
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": "Information",
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "./bridged.log"
                }
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ],
        "Properties": {
            "Application": "BridgeDaemon"
        }
    }
}